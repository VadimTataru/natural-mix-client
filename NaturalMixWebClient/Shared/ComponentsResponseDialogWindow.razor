@using MudBlazor

<MudDialog>
    <DialogContent>
        Dialog Content
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Primary" OnClick="Submit">Ok</MudButton>
    </DialogActions>
</MudDialog>
@code {
    [CascadingParameter] MudDialogInstance ResponseDialogWindow { get; set; }

    void Submit() => ResponseDialogWindow.Close(DialogResult.Ok(true));
    void Cancel() => ResponseDialogWindow.Cancel();
}
